 = () => {\r\n        return (\r\n            listTheater?.map((theater,index) =>{\r\n                // console.log(theater)\r\n                return (\r\n                    <div key={index} className={`col-2 cumrap__item ${isSelected===index ? Active : nonClassActive}`}  \r\n                    onClick={() => (isSelected === index ? '' : chooseTheaterGroup(theater.maHeThongRap,theater.logo, index))}>\r\n                        <img src={theater.logo}/>\r\n                    </div>\r\n                )\r\n            })\r\n        )\r\n    }\r\n    return (\r\n        <section id=\"cumrap\">\r\n            <h3 className=\"title__cumrap\">Cụm rạp</h3>\r\n            <div className=\"row cumrap__main\">\r\n                {renderTheater()}\r\n            </div>\r\n            <div className=\"row chon__rap\">\r\n                <Theater/>\r\n                <TimeMovie/>     \r\n            </div>     \r\n        </section>\r\n    )\r\n}\r\nexport default React.memo(MovieTheater);","E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\constants\\movie.constant.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\constants\\register.constant.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\constants\\theater.constant.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\Carousel\\CarouselData.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\SlideMobie\\DataSlideMobie.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\New\\NewData.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\ReactSlick\\Slickslide.jsx",["298"],"import React,{useEffect} from 'react';\r\nimport {useDispatch} from 'react-redux';\r\nimport Slider from \"react-slick\";\r\nimport { getMovieListAPI } from '..