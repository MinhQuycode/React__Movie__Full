      {\r\n              breakpoint: 768,\r\n              settings: {\r\n                slidesToShow: 2,\r\n                slidesToScroll: 2,\r\n              },\r\n            },\r\n            {\r\n              breakpoint: 480,\r\n              settings: {\r\n                slidesToShow: 1,\r\n                slidesToScroll: 1,\r\n              },\r\n            },\r\n          ],\r\n    };\r\n\r\n    if(props.loading) return <Loading/>\r\n    return (\r\n        <section id=\"slide__movie\">\r\n            <ul className=\"nav nav-pills justify-content-center title\">\r\n                <li className=\"nav-item\">\r\n                    <a className=\"nav-link active\" data-toggle=\"pill\" href=\"#dangChieu\">Đang chiếu</a>\r\n                </li>\r\n                <li className=\"nav-item\">\r\n                    <a className=\"nav-link\" data-toggle=\"pill\" href=\"#sapChieu\">Sắp chiếu</a>\r\n                </li>\r\n            </ul>\r\n            <div className=\"tab-content slideMovie__item\">\r\n                <div className=\"tab-pane active\" id=\"dangChieu\">\r\n                    <Slider {...settings}>\r\n                        {renderListMovie()}\r\n                    </Slider>\r\n                </div>\r\n                <div className=\"tab-pane fade\" id=\"sapChieu\">\r\n                    <Slider {...settings}>\r\n                        {renderListMovie()}\r\n                    </Slider>\r\n                </div>\r\n            </div>\r\n        </section>\r\n    )\r\n}\r\nconst mapStateToProps = (state) =>({\r\n    movieDetail : state.movie.movieList,\r\n    loading : state.movie.loading,\r\n});\r\n\r\nexport default connect(mapStateToProps)(Slickslide);","E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\actions\\theater.action.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\MovieItems\\MovieItems.jsx",["272"],"import React from 'react'\r\nimport sta