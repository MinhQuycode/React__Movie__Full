UEST:\r\n        state.loading = true;\r\n        state.userSignin = null;\r\n        state.error = null;\r\n        return {...state};\r\n\r\n    case SIGN_IN_SUCCESS:\r\n        state.loading = false;\r\n        state.userSignin = payload;\r\n        state.error = null;\r\n        return {...state};\r\n\r\n    case SIGN_IN_FAILED:\r\n        state.loading = false;\r\n        state.userSignin = null;\r\n        state.error = payload;\r\n        return {...state};\r\n    \r\n    case LOG_OUT:\r\n        localStorage.removeItem(\"userLogin\");\r\n        localStorage.clear();\r\n        return { ...initialState};    \r\n\r\n    default:\r\n        return state\r\n    }\r\n}\r\nexport default loginReducer;","E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\reducers\\movie.reducer.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\reducers\\register.reducer.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\reducers\\theater.reducer.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\constants\\login.constant.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\redux\\actions\\register.action.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\Layouts\\Error\\Error.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\Search\\Search.jsx",[],"E:\\Data_2\\XAMPP\\htdocs\\BaiTapCuoiKhoa_DatVeXemPhim\\dv-xemphim\\src\\components\\Home\\Carousel\\CarouselItem.jsx",["291"],"import React, { useState } from 'react'\r\nimport carouselData from './CarouselData';\r\nimport Slider from \"react-slick\";\r\nimport VideoTrailer from '../VideoTrailer/VideoTrailer';\r\n\r\nexport default function CarouselItem() {\r\n    const settings = {\r\n        dots: true,\r\n        infinite: true,\r\n        speed: 2000,\r\n        slidesToShow: 1,\r\n